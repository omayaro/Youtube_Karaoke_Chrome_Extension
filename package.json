{
  "name": "youtube-karaoke-extension",
  "version": "1.0.0",
  "description": "A Brave browser extension for YouTube karaoke with split-panel interface",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "package": "node package-extension.js",
    "package:zip": "npm run package && cd packages && powershell -command \"Compress-Archive -Path 'youtube-karaoke-extension-v1.0.0' -DestinationPath 'youtube-karaoke-extension-v1.0.0.zip' -Force\"",
    "clean": "rimraf dist packages",
    "prepackage": "npm run clean && npm run build"
  },
  "keywords": [
    "browser-extension",
    "youtube",
    "karaoke",
    "brave",
    "chrome"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.45.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.5.0"
  }
}
